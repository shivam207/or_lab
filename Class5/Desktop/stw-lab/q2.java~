import java.util.Scanner;
 public class q2
 {
 public static void main(String[ ] args) 
{ int n,sum=0;
Scanner input = new Scanner(System.in);
System.out.println("enter an integer");
 n = input.nextInt( );
for(int i=0;i<n;i++)
{
if(i%2!=0)
{
sum=sum+i;
}
}
System.out.println("sum of odd numbers less than n="+sum);
}
}


